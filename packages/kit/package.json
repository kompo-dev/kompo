    "clean": "rimraf .turbo node_modules dist build .next coverage tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "eta": "^3.2.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
