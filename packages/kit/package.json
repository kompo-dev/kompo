{
  "name": "@kompo/kit",
  "version": "0.1.2",
  "private": true,
  "description": "Shared interfaces and utilities for Kompo CLI plugins",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
