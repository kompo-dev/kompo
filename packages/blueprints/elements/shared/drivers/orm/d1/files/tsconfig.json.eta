{
  "extends": "<%= it.tsconfigPath %>",
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "declaration": true,
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["src/**/*"]
}
