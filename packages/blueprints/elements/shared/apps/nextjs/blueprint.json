{
  "name": "nextjs",
  "type": "app",
  "env": {
    "NEXT_PUBLIC_APP_NAME": {
      "validation": "z.string()",
      "default": "{{projectName}}",
      "side": "client"
    },
    "NEXT_PUBLIC_HELLO_WORLD": {
      "validation": "z.string()",
      "default": "It Works!",
      "side": "client"
    },
    "NEXT_RUNTIME": {
      "validation": "z.enum(['nodejs', 'edge']).optional()",
      "side": "server"
    }
  },
  "id": "nextjs"
}
