import { describe, it, expect } from 'vitest'
import { create<%= it.pascalName %> } from './<%= it.name %>.entity'

describe('<%= it.pascalName %>', () => {
  it('should create a valid <%= it.pascalName %>', () => {
    // ðŸ”¹ Creation of entity with default values
    const entity = create<%= it.pascalName %>({})


    // ðŸ”¹ ID Verification
    expect(entity.id).toBeDefined()
    
    // ðŸ”¹ Fields Verification
    <% it.fields.forEach(function(field) { %>
    expect(entity.<%= field %>).toBeDefined()
    <% }) %>

    // ðŸ”¹ Type Verification (optional, but educational)
    <% it.fields.forEach(function(field) { %>
    expect(typeof entity.<%= field %>).not.toBe('undefined')
    <% }) %>
  })
})
