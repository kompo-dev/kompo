generator client {
  provider = "prisma-client-js"
}

<%~ include(`libs/drivers/orm/prisma/${it.driver}/snippets/datasource.prisma.eta`, it) %>

// Example model
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
